<?xml version="1.0" encoding="UTF-8"?>

<Coverage xmlns="http://hl7.org/fhir">
  <id value="example-id-coverage"/>
  <meta>
    <profile value="http://alr.cms.gov/ig/StructureDefinition/alr-Coverage"/>
  </meta>
  <text>
    <status value="additional"/><div xmlns="http://www.w3.org/1999/xhtml"><p>General example of a Coverage resource, with an extension for Enrollment periods (ALR enrollment flags)</p></div>
  </text>
  <extension url="http://alr.cms.gov/ig/StructureDefinition/ext-enrollmentFlag">
    <extension url="period">
      <valuePeriod>
        <start value="2020-01-01"/>
        <end value="2020-03-31"/>
      </valuePeriod>
    </extension>
    <extension url="flag">
      <valueCoding>
        <code value="1"/>
        <display value="Medicare enrollment type is ESRD"/>
      </valueCoding>
    </extension>
  </extension>
  <extension url="http://alr.cms.gov/ig/StructureDefinition/ext-enrollmentFlag">
    <extension url="period">
      <valuePeriod>
        <start value="2020-04-01"/>
        <end value="2020-12-31"/>
      </valuePeriod>
    </extension>
    <extension url="flag">
      <valueCoding>
        <code value="2"/>
        <display value="Medicare enrollment type is Disabled"/>
      </valueCoding>
    </extension>
  </extension>
  <extension url="http://alr.cms.gov/ig/StructureDefinition/ext-partDMonths">
    <valueInteger value="5"/>
  </extension>
  <status value="active"/>
  <subscriberId value="-19999000381"/>
  <beneficiary>
    <reference value="Patient/example-id-patient"/>
  </beneficiary>
  <period>
    <start value="2020-01-01"/>
    <end value="2020-12-31"/>
  </period>
  <payor>
    <reference value="Organization/123"/>
  </payor>
</Coverage>